{
  "name": "voice-ai-poc",
  "version": "0.1.0",
  "description": "Voice AI Prospecting System - Proof of Concept",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "MODE=telnyx tsx watch src/index.ts",
    "dev:twilio": "MODE=twilio tsx watch src/index.ts",
    "local": "MODE=local tsx src/scripts/local-test.ts",
    "twilio:test": "MODE=twilio tsx src/scripts/twilio-test.ts",
    "test": "jest",
    "test:latency": "tsx src/tests/latency-benchmark.ts",
    "test:elevenlabs": "tsx src/scripts/test-elevenlabs.ts",
    "test:mic": "tsx src/scripts/test-microphone.ts",
    "test:scribe": "tsx src/scripts/test-scribe.ts",
    "preload-fillers": "tsx src/scripts/preload-fillers.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.29.0",
    "@ricky0123/vad-node": "^0.0.3",
    "dotenv": "^17.2.3",
    "node-record-lpcm16": "^1.0.1",
    "openai": "^6.15.0",
    "speaker": "^0.5.5",
    "telnyx": "^4.6.0",
    "uuid": "^13.0.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "voice-ai",
    "tts",
    "stt",
    "llm",
    "telephony",
    "prospecting"
